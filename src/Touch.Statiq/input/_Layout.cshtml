<!doctype html>
<html class="no-js" lang="nl-be">
  <head>
    <meta charset="utf-8">
    <meta name="description" content="TTC Touch is een tafeltennisclub uit Wommelgem, die met 4 ploegen deelneemt aan de competitie van de KAVVV vriendenclubs.">
    <meta name="keywords" content="ttc touch,touch,tafeltennis,ping pong,wommelgem,avvv,kavvv,fort,fort 2">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="apple-touch-icon" href="apple-touch-icon.png">
    <!-- Place favicon.ico in the root directory -->

    <link rel="stylesheet" href="styles/main.css">
    <title>@(ViewBag.title ?? "TTC Touch")</title>
  </head>
  <body>
    <div class="container">
        <div class="header mt-2 mb-3">
          <ul class="nav nav-pills nav-fill">
              @{
                    var active = ViewBag.active;
                    Func<string, string> navLinkClass = (string page) => page == active ? "nav-link active" : "nav-link";
                }
            <li class="nav-item"><a class="@(navLinkClass("home"))" href="./"><span class="fas fa-home"></span> TTC Touch</a></li>
            <li class="nav-item"><a class="@(navLinkClass("geschiedenis"))" href="geschiedenis.html"><span class="fas fa-book"></span> Clubgeschiedenis</a></li>
            <li class="nav-item"><a class="@(navLinkClass("lokaal"))" href="lokaal.html"><span class="fas fa-map-marker-alt"></span> Lokaal</a></li>
            <li class="nav-item"><a class="@(navLinkClass("contact"))" href="contact.html"><span class="fas fa-envelope"></span> Contact</a></li>
            <li class="nav-item"><a class="@(navLinkClass("kavvv"))" href="kavvv.html"><span class="fas fa-table-tennis"></span> KAVVV</a></li>
          </ul>
<!--          <h3 class="text-muted">TTC Touch</h3> -->
        </div>

        @RenderBody()
    </div>

<!-- Google Analytics: change UA-XXXXX-X to be your site's ID. -->
